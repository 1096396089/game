{
  "name": "server",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "build": "nest build",
    "format": "prettier --write \"apps/**/*.ts\" \"libs/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/apps/server/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./apps/server/test/jest-e2e.json",
    "push": "cd .. && git add -A && git commit -am 'build:build'&&git push",
    "admin": "rm -rf ./public/admin/ && cd ../admin && npm run build && cd ../server",
    "server": "rm -rf prod && cp -rf dist prod",
    "web": "rm -rf ./public/web/ && cd ../investor && npm run build-only && cd ../server"
  },
  "dependencies": {
    "@binance/connector": "^3.6.1",
    "@nestjs/cache-manager": "^1.0.0",
    "@nestjs/common": "^9.0.0",
    "@nestjs/config": "^3.0.0",
    "@nestjs/core": "^9.0.0",
    "@nestjs/jwt": "^10.0.3",
    "@nestjs/mongoose": "^11.0.2",
    "@nestjs/passport": "^9.0.3",
    "@nestjs/platform-express": "^9.0.0",
    "@nestjs/schedule": "^4.1.1",
    "@nestjs/swagger": "^6.3.0",
    "@nestjs/typeorm": "^11.0.0",
    "@typegoose/typegoose": "^11.1.0",
    "@types/mongoose": "^5.11.97",
    "@types/multer": "^1.4.7",
    "@types/nodemailer": "^6.4.17",
    "ali-oss": "^6.17.1",
    "aliyun-sdk": "^1.12.10",
    "axios": "^1.9.0",
    "bcryptjs": "^2.4.3",
    "binance-api-node": "^0.12.9",
    "cache-manager": "^5.2.1",
    "crypto": "^1.0.1",
    "crypto-js": "^4.2.0",
    "esdk-obs-nodejs": "^3.24.3",
    "handlebars": "^4.7.8",
    "http-proxy-agent": "^4.0.1",
    "https-proxy-agent": "^5.0.1",
    "i": "^0.3.7",
    "mongoose": "^8.12.1",
    "multer-huawei-obs": "^1.0.0",
    "mysql2": "^3.14.1",
    "nestjs-mongoose-crud": "^2.1.2",
    "nestjs-telegraf": "^2.9.1",
    "nestjs-typegoose": "^7.1.38",
    "nodemailer": "^7.0.3",
    "npm": "^11.3.0",
    "obs-upload-checkpoint": "^1.0.8",
    "passport": "^0.6.0",
    "passport-jwt": "^4.0.1",
    "passport-local": "^1.0.0",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^7.2.0",
    "sax": "^1.2.4",
    "socks-proxy-agent": "^8.0.5",
    "swagger-ui-express": "^4.6.3",
    "technicalindicators": "^3.1.0",
    "telegraf": "^4.16.3",
    "typeorm": "^0.3.24",
    "ws": "^8.18.2",
    "xmlbuilder": "^15.1.1"
  },
  "devDependencies": {
    "@nestjs/cli": "^9.0.0",
    "@nestjs/schematics": "^9.0.0",
    "@nestjs/testing": "^9.0.0",
    "@types/express": "^4.17.13",
    "@types/jest": "29.2.4",
    "@types/node": "18.11.18",
    "@types/supertest": "^2.0.11",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.0.0",
    "eslint": "^8.0.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "jest": "29.3.1",
    "prettier": "^2.3.2",
    "source-map-support": "^0.5.20",
    "supertest": "^6.1.3",
    "ts-jest": "29.0.3",
    "ts-loader": "^9.2.3",
    "ts-node": "^10.0.0",
    "tsconfig-paths": "4.1.1",
    "typescript": "^4.7.4"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": ".",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "./coverage",
    "testEnvironment": "node",
    "roots": [
      "<rootDir>/apps/",
      "<rootDir>/libs/"
    ],
    "moduleNameMapper": {
      "^@app/connom(|/.*)$": "<rootDir>/libs/connom/src/$1",
      "^@app/db(|/.*)$": "<rootDir>/libs/db/src/$1",
      "^@app/mysql_db(|/.*)$": "<rootDir>/libs/mysql_db/src/$1",
      "^@app/mail(|/.*)$": "<rootDir>/libs/mail/src/$1",
      "^@app/telegraf-bot(|/.*)$": "<rootDir>/libs/telegraf-bot/src/$1"
    }
  }
}
